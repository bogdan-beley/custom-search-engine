@model CustomSearchEngine.Models.SearchResult

@{
    ViewData["Title"] = "Search Results";
}

@await Html.PartialAsync("_SearchPartial")

@foreach (var item in Model.SearchResultItems)
{
    <p><a href="@item.Link" target="_blank">@item.Link</a></p>
    <p class="h5">@item.Title</p>
    <p class="font-italic">@item.Snippet</p>
    <br />
}