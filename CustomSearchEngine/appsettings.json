{
    "ConnectionStrings": {
        "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=CustomSearchEngineDb;Trusted_Connection=True;"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },

    "AzureKeyVault": {
        "BaseUrl": "https://customsearchenginekeys.vault.azure.net/"
    },

    "ExternalApiClientsConfig": {
        "BingWebSearchApiClient": {
            "Url": "https://api.bing.microsoft.com/v7.0/search/?q="
        },
        "GoogleWebSearchApiClient": {
            "Url": "https://www.googleapis.com/customsearch/v1"
        }
    },
    "AllowedHosts": "*"
}
